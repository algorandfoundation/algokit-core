{
  "description": "This package.json contains the workspaces for scripts and the dependencies for semantic-release",
  "workspaces": [
    "scripts/build"
  ],
  "type": "module",
  "scripts": {
    "build:algokit_transact:py": "bun scripts/build algokit_transact python",
    "build:algokit_transact:swift": "bun scripts/build algokit_transact swift",
    "build:algokit_transact:ts": "bun scripts/build algokit_transact typescript",
    "build:algokit_transact:all": "bun scripts/build algokit_transact all",
    "docs:build": "cargo run --bin build-docs --manifest-path docs/Cargo.toml",
    "docs:serve": "python3 -m http.server --directory target/doc/ 8000",
    "docs:dev": "cargo run --bin build-docs --manifest-path docs/Cargo.toml && python3 -m http.server --directory target/doc/ 8000"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@semantic-release/changelog": "^6.0.3",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "ms": "^2.1.3",
    "semantic-release": "^24.2.3",
    "semantic-release-gha-output": "^1.1.1",
    "semantic-release-scope-filter": "1.0.0"
  }
}
